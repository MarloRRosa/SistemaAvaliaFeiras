<div class="p-6">
    <h1 class="text-2xl font-bold mb-4">Sugest√µes e Feedbacks</h1>

    <% if (feedbacks.length === 0) { %>
        <p class="text-gray-500">Nenhum feedback enviado ainda.</p>
    <% } else { %>
        <div class="space-y-4">
            <% feedbacks.forEach(feedback => { %>
                <div class="bg-white shadow rounded p-4 border border-gray-200">
                    <p><strong>Categoria:</strong> <%= feedback.categoria %></p>
                    <p><strong>Mensagem:</strong> <%= feedback.mensagem %></p>
                    <% if (feedback.nome) { %>
                        <p><strong>Nome:</strong> <%= feedback.nome %></p>
                    <% } %>
                    <% if (feedback.email) { %>
                        <p><strong>Email:</strong> <%= feedback.email %></p>
                    <% } %>
                    <p class="text-sm text-gray-500 mt-2">Enviado em: <%= new Date(feedback.createdAt).toLocaleString() %></p>
                </div>
            <% }); %>
        </div>
    <% } %>
</div>
