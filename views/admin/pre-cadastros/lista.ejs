<h2 class="text-2xl font-bold mb-4">Pré-Cadastros Recebidos</h2>
<table class="min-w-full bg-white border rounded shadow-sm">
  <thead class="bg-gray-100">
    <tr>
      <th class="px-4 py-2 text-left">Nome</th>
      <th class="px-4 py-2 text-left">Email</th>
      <th class="px-4 py-2 text-left">Telefone</th>
      <th class="px-4 py-2 text-left">Ações</th>
    </tr>
  </thead>
  <tbody>
    <% preCadastros.forEach(pre => { %>
      <tr class="border-b">
        <td class="px-4 py-2"><%= pre.nome %></td>
        <td class="px-4 py-2"><%= pre.email %></td>
        <td class="px-4 py-2"><%= pre.telefone || '-' %></td>
        <td class="px-4 py-2">
          <a href="/admin/pre-cadastros/<%= pre._id %>" class="text-blue-600 hover:underline">Ver / Aprovar</a>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
